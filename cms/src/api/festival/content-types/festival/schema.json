    },
    "description": {
      "type": "text",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "date": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    },
    "tickets": {
      "type": "string"
    },
    "music_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::music-type.music-type",
      "inversedBy": "festivals"
    },
    "place": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::place.place",
      "inversedBy": "festivals"
