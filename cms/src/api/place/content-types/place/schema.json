{
  "kind": "collectionType",
  "collectionName": "places",
  "info": {
    "singularName": "place",
    "pluralName": "places",
    "displayName": "Place",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "lat": {
      "type": "float",
      "required": false
    },
    "lng": {
      "type": "float",
      "required": false
    },
    "city": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "festivals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::festival.festival",
      "mappedBy": "place"
    },
    "geolocation": {
      "type": "customField",
      "options": {
        "info": true
      },
      "customField": "plugin::geodata.geojson"
    },
    "geohash": {
      "type": "string"
    }
  }
}
